[color]
	status = auto
	branch = auto
	diff = auto
	interactive = auto
[core]
	excludesfile = /Volumes/Home/pharkas/.gitignore
	editor = vim
[clean]
    requireForce = false
[alias]
    pr = pull --rebase
    mf = merge --no-ff
    lol = log --pretty=oneline --abbrev-commit --graph --decorate
    lola = log --pretty=oneline --abbrev-commit --graph --decorate --all
    visual = !~/Applications/GitX.app/Contents/Resources/gitx
	v = !~/Applications/GitX.app/Contents/Resources/gitx
    c = checkout
    co = checkout
    ca = commit -a --amend -C HEAD
    cam = commit -a -S -m
    cma = commit -a -S -m
    resign = commit --amend -S
    dm = diff master
	id = log -n1 --pretty=format:%h
    cherry-pick = cherry-pick -x
    s = status --untracked-files --short --branch
    st = status --untracked-files --short --branch
    standup = log --since yesterday --author Jacob
